<%- include('game-admin-header') -%>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <main>
        <button id="plus" value="plus">PLUS</button>
        <button id="minus" value="minus">MINUS</button>
        <button id="multiply" value="multiply">MULTIPLY</button>
        <button id="division" value="division">DIVISION</button>
        <p id="json"></p>
    </main>


<script>
    $('#plus').on('click', function () {
        $.ajax({
            type: "GET",
            dataType: 'json',
            url: "../../game-math1/targil/plus",
            data: {
                id: "somedata"
            },
            success: function (result) {
                $('#json').html('<pre>' + JSON.stringify(result, null, 2) + '</pre>');
            },
            error: function (result) {
                alert('error');
            }
        });
    });
</script>

