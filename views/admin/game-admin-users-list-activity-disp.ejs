<h3>ACTIVITY</h3>

<pre id="whereToPrint"></pre>

<div class="divTable blueTable">
    <div class="divTableHeading">
      <div class="divTableRow">
          <div class="divTableHead">date_creation</div>
          <div class="divTableHead">targil</div>
          <div class="divTableHead">duration (msec)</div>
          <div class="divTableHead">number_of_errors</div>
          <div class="divTableHead">finish</div>
      </div>
    </div>
    <div id=tablebody class="divTableBody">
    </div>
</div>
