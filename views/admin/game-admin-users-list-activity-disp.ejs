<body onload="funStart()">

<!-- present the list of information -->
<script>
function htmlDecode(input){
  var e = document.createElement('div');
  e.innerHTML = input;
  return e.childNodes.length === 0 ? "" : e.childNodes[0].nodeValue;
}
function funStart() {

  var targil_info_all =  JSON.parse(htmlDecode("<%= JSON.stringify(targil_info) %>"));
  
  targil_info_all.forEach(targil_info => {

    var para = document.createElement('div');
    para.setAttribute('class', 'divTableRow');

    var t = document.createElement('div');
    t.setAttribute('class', 'divTableCell');
    var tt = document.createTextNode(targil_info.date_creation);
    t.appendChild(tt);
    para.appendChild(t);

    var t = document.createElement('div');
    t.setAttribute('class', 'divTableCell');
    var tt = document.createTextNode(targil_info.targil);
    t.appendChild(tt);
    para.appendChild(t);

    var t = document.createElement('div');
    t.setAttribute('class', 'divTableCell');
    var tt = document.createTextNode(targil_info.targil_duration_msec);
    t.appendChild(tt);
    para.appendChild(t);
            
    var t = document.createElement('div');
    t.setAttribute('class', 'divTableCell');
    var tt = document.createTextNode(targil_info.targil_number_of_errors);
    t.appendChild(tt);
    para.appendChild(t);
            
    var t = document.createElement('div');
    t.setAttribute('class', 'divTableCell');
    var tt = document.createTextNode(targil_info.targil_flag_finish);
    t.appendChild(tt);
    para.appendChild(t);

    document.getElementById("tablebody").appendChild(para);
  });
};



